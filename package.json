{
  "name": "@geckoai/class-transformer",
  "version": "1.0.2",
  "description": "The class-transformer is used to convert a plain object to a class instance.",
  "main": "dist/cjs",
  "types": "dist/types",
  "scripts": {
    "build:clean": "rimraf dist && rimraf docs",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "build:umd": "webpack --config ./webpack.config.js",
    "build:doc": "typedoc --options ./typedoc.json",
    "release": "yarn build:clean &&  yarn build:cjs && yarn build:umd  && yarn build:types &&  yarn  build:doc",
    "test": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geckoai/class-transformer.git"
  },
  "keywords": [
    "geckoai",
    "class",
    "class transformer"
  ],
  "author": "RanYunLong<549510622@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geckoai/class-transformer/issues"
  },
  "homepage": "https://github.com/geckoai/class-transformer#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.11.3",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "@eslint/js": "^9.13.0",
    "typescript-eslint": "^8.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "typescript": "~5.6.2",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.1",
    "typedoc": "^0.26.10",
    "uuid": "^8.3.2",
    "@types/uuid": "^8.3.4",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@geckoai/class-mirror": "1.0.2",
    "@types/validator": "^13.7.1",
    "validator": "^13.7.0"
  }
}
